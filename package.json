{
	"name": "xlsx-reader",
	"version": "0.0.1",
	"author": "docFoo",
	"description": "Excel (XLSB/XLSX/XLS/XML) ODS and other spreadsheet format (CSV/DIF/DBF/SYLK) parser and writer",
	"keywords": [ "excel", "xls", "xlsx", "xlsb", "xlsm", "ods", "csv", "dbf", "dif", "sylk", "office", "spreadsheet" ],
	"bin": {
		"xlsx-reader": "./"
	},
	"main": "./xlsx-reader",
	"types": "types",
	"browser": {
		"node": false,
		"crypto": false,
		"stream": false,
		"process": false,
		"fs": false
	},
	"dependencies": {
    "xlsx": "~0.8.0",
    "angular": "~1.3.15",
    "bootstrap": "~3.3.4",
    "jquery": "~2.1.3",
    "lodash": "~3.7.0"
	},
	"devDependencies": {
	},
	"repository": { "type":"git", "url":"git://github.com/Massimo-Dacadoo/xlsx-reader.js.git" },
	"scripts": {
		"pretest": "git submodule init && git submodule update"
	},
	"config": {
		"blanket": {
			"pattern": "xlsx.js"
		}
	},
	"homepage": "https://github.com/Massimo-Dacadoo/xlsx-reader.js",
	"bugs": { "url": "ttps://github.com/Massimo-Dacadoo/xlsx-reader.js/issues" },
	"license": "Apache-2.0",
	"engines": { "node": ">=0.8" }
}